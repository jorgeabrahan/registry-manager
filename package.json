{
  "name": "registry-manager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "tailwind": "bun tailwindcss -i ./src/styles/input.css -o ./src/styles/output.css --watch",
    "preview": "vite preview",
    "init": "bun run dev & bun run tailwind"
  },
  "dependencies": {
    "firebase": "10.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sonner": "2.0.1",
    "uuid": "9.0.1",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "18.2.56",
    "@types/react-dom": "18.2.19",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "7.9.0",
    "@typescript-eslint/parser": "7.9.0",
    "@vitejs/plugin-react-swc": "3.5.0",
    "eslint": "8.56.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "prop-types": "15.8.1",
    "tailwindcss": "3.4.1",
    "typescript": "5.4.5",
    "vite": "5.1.4"
  },
  "module": "index.ts"
}
